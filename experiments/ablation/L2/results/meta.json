{
  "exp_id": "L2_cccp",
  "seed": 42,
  "dataset": {
    "name": "cifar100",
    "batch_size": 128,
    "num_workers": 4,
    "data_aug": 1
  },
  "teachers": [
    {
      "name": "resnet152",
      "ckpt": "checkpoints/teachers/resnet152_cifar100.pth",
      "test_acc": 79.26
    },
    {
      "name": "convnext_s",
      "ckpt": "checkpoints/teachers/convnext_s_cifar100.pth",
      "test_acc": 68.65
    }
  ],
  "student": {
    "name": "mobilenet_v2_scratch",
    "pretrained": false,
    "use_adapter": true
  },
  "kd": {
    "method": "asib_ablation_stage",
    "ce_alpha": 0.7,
    "kd_alpha": 0.3,
    "kd_ens_alpha": 0.0,
    "kd_warmup_epochs": 10
  },
  "ib": {
    "use_ib": true,
    "ib_beta": 5e-05,
    "ib_epochs_per_stage": 6,
    "ib_mbm_out_dim": 256,
    "ib_mbm_n_head": 4
  },
  "cccp": {
    "use_cccp": false
  },
  "optim": {
    "optimizer": "sgd",
    "student_lr": 0.1,
    "student_weight_decay": 0.0005,
    "grad_clip_norm": 0
  },
  "schedule": {
    "type": "cosine",
    "lr_warmup_epochs": 5,
    "min_lr": 1e-05,
    "step_size": null,
    "gamma": null
  },
  "ema": {
    "use_ema_teacher": false
  },
  "cl": {
    "cl_mode": false,
    "cl_method": null
  },
  "amp": {
    "use_amp": true,
    "amp_dtype": "bfloat16"
  },
  "ppf": {
    "use_partial_freeze": false,
    "use_teacher_finetuning": false,
    "student_freeze_level": -1,
    "teacher1_freeze_level": -1,
    "teacher2_freeze_level": -1,
    "student_freeze_bn": false,
    "teacher1_freeze_bn": true,
    "teacher2_freeze_bn": true
  },
  "tadapt": true
}