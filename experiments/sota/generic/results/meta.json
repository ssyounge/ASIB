{
  "exp_id": "sota_generic",
  "seed": 42,
  "dataset": {
    "name": "cifar100",
    "batch_size": 128,
    "num_workers": 4,
    "data_aug": 1
  },
  "teachers": [
    {
      "name": "resnet152",
      "ckpt": null,
      "test_acc": 0.0
    },
    {
      "name": "convnext_s",
      "ckpt": null,
      "test_acc": 0.0
    }
  ],
  "student": {
    "name": "mobilenet_v2_scratch",
    "pretrained": false,
    "use_adapter": true
  },
  "kd": {
    "method": "vanilla_kd",
    "ce_alpha": 0.65,
    "kd_alpha": 0.35,
    "kd_ens_alpha": 0.0,
    "teacher_adapt_kd_warmup": 0
